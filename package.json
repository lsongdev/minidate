{
  "name": "minidate",
  "version": "0.0.1",
  "main": "./dist/minidate.js",
  "scripts": {
    "test": "node test",
    "build:umd": "NODE_ENV=umd rollup -c",
    "build:cjs": "NODE_ENV=cjs rollup -c",
    "build": "npm run build:cjs && npm run build:umd",
    "build:docs": "jsdoc -c jsdoc.json",
    "build:docs:watch": "npx nodemon --exec 'npx jsdoc -c jsdoc.json' --watch src/*.js"
  },
  "description": "Minimal Date Library for JavaScript and Node.js",
  "author": "lsong",
  "license": "MIT",
  "keywords": [
    "date",
    "time",
    "datetime",
    "tinydate",
    "moment"
  ],
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/song940/minidate.git"
  },
  "bugs": {
    "url": "https://github.com/song940/minidate/issues"
  },
  "homepage": "https://blog.lsong.org/minidate",
  "devDependencies": {
    "docdash": "^1.1.1",
    "jsdoc": "^3.6.3",
    "rollup": "^1.15.4",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-license": "^0.9.0",
    "rollup-plugin-uglify-es": "^0.0.1"
  }
}
